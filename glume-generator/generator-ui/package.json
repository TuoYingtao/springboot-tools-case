    "lodash": "^4.17.21",
    "nprogress": "0.2.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persist": "^1.0.0",
    "qrcode.vue": "^3.2.2",
    "qs": "^6.10.5",
    "sortablejs": "^1.15.0",
    "tvision-color": "^1.3.1",
    "v-viewer": "^3.0.10",
    "vue": "^3.3.4",
