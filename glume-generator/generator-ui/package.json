{
  "name": "vue3-system-starter-template",
  "version": "1.0.0",
  "author": "TuoYingtao",
  "description": "Vue3 + Vite + Typescript 后台管理启动框架",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "dev:linux": "vite --mode development",
    "production": "vite --open --mode production",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build --mode production",
    "build:test": "vite build --mode production",
    "build:release": "vue-tsc --noEmit --skipLibCheck && vite build --mode release",
    "build-only": "vite build",
    "build:check": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "format": "prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/XXXXXX/XXXXX.git"
  },
